{

	"manifest_version": 2,
	"name": "Chronos",
	"version": "1.0",
	"description": "Video/Audio playback speed regulator.",
	"permissions": ["tabs", "activeTab", "webNavigation", "*://*/*"],
	"background": {
		"scripts": ["./src/background.js"]

	},
	"content_scripts": [{
		"run_at": "document_idle",
		"matches": ["<all_urls>"],
		"js": ["./src/contentscript.js"]
	}],
	"browser_action": {
		"browser_style": true,
		"default_icon": "./img/chronos_icon_light.png",
		"theme_icons": [{
			"light": "./img/chronos_icon_light.png",
			"dark": "./img/chronos_icon_dark.png",
			"size": 16
		}, {
			"light": "./img/chronos_icon_light.png",
			"dark": "./img/chronos_icon_dark.png",
			"size": 32
		}],
		"default_popup": "./popup.html",
		"default_title": "Chronos extension"
	},
	"commands": {
		"setMarker": {
			"suggested_key": {
				"default": "Alt+K",
				"mac": "Alt+K"
			},
			"description": "Create timestamp marker. NOOO lel"
		}
	}
}