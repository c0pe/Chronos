{
	"name": "Chronos",
	"version": "0.1",
	"description": "Video/Audio playback speed regulator and timestamp marker.",
	"permissions": ["declarativeContent", "storage"],
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["./src/content.js"]
	}],
	"background": {
		"scripts": ["./src/background.js"],
		"persistent": false
	},
	"browser_action":{
		"default_icon": "./img/chronos_icon.png"
	},
	"manifest_version": 2
}